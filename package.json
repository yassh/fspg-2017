{
  "name": "fspg-2017",
  "version": "0.0.0",
  "author": "yassh <yassh32@outlook.jp>",
  "dependencies": {
    "nuxt": "^1.4.2",
    "papaparse": "^4.6.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "fixpack": "^2.3.1",
    "flow-bin": "^0.81.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "stylelint": "^9.5.0",
    "stylelint-order": "^1.0.0"
  },
  "license": "MIT",
  "repository": "https://github.com/yassh/fspg-2017.git",
  "scripts": {
    "build": "nuxt generate",
    "dev": "nuxt",
    "fix": "npm-run-all -p fix:*",
    "fix:css": "npm-run-all fix:css:prettier fix:css:stylelint",
    "fix:css:prettier": "prettier './**/*.css' --ignore-path .lintignore --write",
    "fix:css:stylelint": "stylelint './**/*.css' --ignore-path .lintignore --fix",
    "fix:js": "eslint './**/*.{js,vue}' --ignore-path .lintignore --fix",
    "fix:pack": "fixpack",
    "lint": "npm-run-all lint:* typecheck",
    "lint:css": "stylelint './**/*.css' --ignore-path .lintignore",
    "lint:js": "eslint './**/*.{js,vue}' --ignore-path .lintignore",
    "test": "jest",
    "typecheck": "flow"
  }
}
